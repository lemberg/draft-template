# Ignore non-custom parts of Drupal.
/docroot/core/**
/docroot/modules/contrib/**
/docroot/profiles/contrib/**
/docroot/themes/contrib/**

# Ignore files added by Drupal Scaffold Composer plugin.
/.gitattributes
/.editorconfig
/docroot/.csslintrc
/docroot/.eslintignore
/docroot/.eslintrc.json
/docroot/.htaccess
/docroot/.ht.router.php
/docroot/autoload.php
/docroot/example.gitignore
/docroot/index.php
/docroot/robots.txt
/docroot/update.php
/docroot/web.config
/docroot/sites/README.txt
/docroot/sites/development.services.yml
/docroot/sites/example.settings.local.php
/docroot/sites/example.sites.php
/docroot/sites/default/default.services.yml
/docroot/sites/default/default.settings.php
/docroot/modules/README.txt
/docroot/profiles/README.txt
/docroot/themes/README.txt

# Ignore local setup.
/docroot/sites/default/development.services.yml
/docroot/sites/default/settings.local.php

# Ignore paths that contain user-generated content.
/docroot/sites/*/files/**
/docroot/sites/*/private/**

# Ignore SimpleTest multi-site environment.
/docroot/sites/simpletest/**

# Ignore Composer-managed dependencies.
/vendor/**

# Ignore Vagrant virtual machine data.
/.vagrant/**

# Ignore local VM settings.
/vm-settings.local.yml

# Ignore PHPUnit results cache.
/.phpunit.result.cache

# Ignore test configuration overrides.
/grumphp.yml
/phpcs.xml
/phpstan.neon
/phpunit.xml

# Ignore .env files.
/.env
